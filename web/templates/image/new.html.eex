<h2>Uplad new image</h2>

<%= form_for @changeset, image_path(@conn, :create), [multipart: true], fn f -> %>

  <div class="form-group">
    <label for="">Image</label>
    <%= file_input f, :image %>
    <%= error_tag f, :image %>
  </div>
  <div class="form-group">
    <%= submit "Submit", class: "btn btn-default" %>
  </div>
<% end %>
